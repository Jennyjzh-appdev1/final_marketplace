<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Item type #<%= @item_type.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/item_types" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/item_types/<%= @item_type.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit item type
        </a>
      </div>

      <div class="col">
        <a href="/delete_item_type/<%= @item_type.id %>" class="btn btn-block btn-outline-secondary">
          Delete item type
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@item_type.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@item_type.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "item_type_templates/item_type_items" %>
  </div>
</div>
